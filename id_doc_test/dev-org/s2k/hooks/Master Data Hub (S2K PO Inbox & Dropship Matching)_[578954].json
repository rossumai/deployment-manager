{
  "id": 578954,
  "type": "webhook",
  "name": "Master Data Hub (S2K PO Inbox & Dropship Matching)",
  "url": "https://us.api.rossum.ai/v1/hooks/578954",
  "description": "Enhance the extracted data with details from your master records.",
  "settings": {
    "configurations": [
      {
        "name": "PO matching",
        "source": {
          "dataset": "s2k_purchase_orders",
          "queries": [
            {
              "aggregate": [
                {
                  "$match": {
                    "$and": [
                      {
                        "PO Number": "{order_id_normalized}"
                      },
                      {
                        "PO Number": {
                          "$ne": ""
                        }
                      }
                    ]
                  }
                }
              ]
            }
          ]
        },
        "default": {
          "label": "---",
          "value": ""
        },
        "mapping": {
          "dataset_key": "PO Number",
          "target_schema_id": "order_match"
        },
        "result_actions": {
          "no_match_found": {
            "selected": "default"
          },
          "one_match_found": {
            "select": "best_match"
          },
          "multiple_matches_found": {
            "select": "best_match"
          }
        },
        "additional_mappings": [
          {
            "dataset_key": "PO Type",
            "target_schema_id": "dropship_flag_match"
          }
        ]
      }
    ]
  },
  "active": true,
  "events": [
    "annotation_content.initialize",
    "annotation_content.started",
    "annotation_content.updated"
  ],
  "queues": [
    "https://us.api.rossum.ai/v1/queues/1740142",
    "https://us.api.rossum.ai/v1/queues/1747529"
  ],
  "run_after": [],
  "metadata": {},
  "config": {
    "private": true,
    "schedule": {},
    "app": null,
    "payload_logging_enabled": false,
    "timeout_s": 60,
    "max_polling_time_s": 300,
    "retry_count": 4,
    "retry_after_polling_failure": true,
    "insecure_ssl": false,
    "client_ssl_certificate": null,
    "retry_on_any_non_2xx": false
  },
  "test": {},
  "sideload": [
    "schemas"
  ],
  "settings_schema": null,
  "secrets_schema": {
    "type": "object",
    "additionalProperties": {
      "type": "string"
    }
  },
