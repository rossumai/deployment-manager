{
  "id": 670261,
  "type": "webhook",
  "name": "Master Data Hub (S2K PO Terms Matching)",
  "url": "https://us.api.rossum.ai/v1/hooks/670261",
  "description": "Enhance the extracted data with details from your master records.",
  "settings": {
    "configurations": [
      {
        "name": "Vendor Payment Terms Match",
        "source": {
          "dataset": "payment_terms",
          "queries": [
            {
              "//": "Match from vendor master data",
              "find": {
                "Payment Terms": "{selected_vendor_terms_id}"
              }
            }
          ]
        },
        "default": {
          "label": "---",
          "value": ""
        },
        "mapping": {
          "dataset_key": "\"Payment Terms\"",
          "label_template": "{\"Description\"}",
          "target_schema_id": "vendor_terms_match"
        },
        "result_actions": {
          "no_match_found": {
            "select": "default"
          },
          "one_match_found": {
            "select": "best_match"
          },
          "multiple_matches_found": {
            "select": "best_match",
            "message": {
              "type": "warning",
              "content": "Multiple matches found"
            }
          }
        },
        "additional_mappings": [
          {
            "dataset_key": "Description",
            "target_schema_id": "vendor_terms_description"
          },
          {
            "dataset_key": "\"Discount %\"",
            "target_schema_id": "vendor_discount_size"
          },
          {
            "dataset_key": "Days to Pay Discount",
            "target_schema_id": "vendor_discount_period"
          },
          {
            "dataset_key": "Day of Month to Discount",
            "target_schema_id": "vendor_discount_day_of_month"
          },
          {
            "dataset_key": "\"Discount Beginning Month C/N\"",
            "target_schema_id": "vendor_discount_beginning_month"
          },
          {
            "dataset_key": "Days to Pay Net",
            "target_schema_id": "vendor_net_period"
          },
          {
            "dataset_key": "Day of Month to Net",
            "target_schema_id": "vendor_net_day_of_month"
          },
          {
            "dataset_key": "\"Net Beginning Month C/N\"",
            "target_schema_id": "vendor_net_beginning_month"
          }
        ]
      }
    ]
  },
  "active": true,
  "events": [
    "annotation_content.initialize",
    "annotation_content.started",
    "annotation_content.updated"
  ],
  "queues": [
    "https://us.api.rossum.ai/v1/queues/1771791"
  ],
  "run_after": [
    "https://us.api.rossum.ai/v1/hooks/592634",
    "https://us.api.rossum.ai/v1/hooks/593147"
  ],
  "metadata": {},
  "config": {
    "private": true,
    "schedule": {},
    "app": null,
    "payload_logging_enabled": false,
    "timeout_s": 60,
    "max_polling_time_s": 300,
    "retry_count": 4,
    "retry_after_polling_failure": true,
    "insecure_ssl": false,
    "client_ssl_certificate": null,
    "retry_on_any_non_2xx": false
  },
  "test": {},
  "sideload": [
    "schemas"
  ],
  "settings_schema": null,
  "secrets_schema": {
    "type": "object",
    "additionalProperties": {
      "type": "string"
    }
  },
  "token_owner": "https://us.api.rossum.ai/v1/users/383208",
