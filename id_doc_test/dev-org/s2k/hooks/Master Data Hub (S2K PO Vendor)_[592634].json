  "description": "Enhance the extracted data with details from your master records.",
  "settings": {
    "configurations": [
      {
        "name": "Invoice vendor matching",
        "source": {
          "dataset": "vendors",
          "queries": [
            {
              "aggregate": [
                {
                  "$match": {
                    "$and": [
                      {
                        "Supplier Number": "{sender_id}"
                      },
                      {
                        "Supplier Number": {
                          "$ne": ""
                        }
                      }
                    ]
                  }
                }
              ]
            },
            {
              "//": "Match by name and address and phone",
              "aggregate": [
                {
                  "$search": {
                    "compound": {
                      "must": [
                        {
                          "text": {
                            "path": [
                              "Name"
                            ],
                            "fuzzy": {
                              "maxEdits": 1
                            },
                            "query": [
                              "{sender_name}"
                            ]
                          }
                        }
                      ],
                      "should": [
                        {
                          "text": {
                            "path": [
                              "Primary Address Street1",
                              "Primary Address Street2",
                              "Primary Address City"
                            ],
                            "fuzzy": {
                              "maxEdits": 1
                            },
                            "query": [
                              "{sender_address}"
                            ]
                          }
                        }
                      ]
                    }
                  }
                },
                {
                  "$match": {
                    "Phone Number": "{sender_phone_normalized}"
                  }
                },
                {
                  "$limit": 10
                },
                {
                  "$addFields": {
                    "__score": {
                      "$meta": "searchScore"
                    }
                  }
                },
                {
                  "$addFields": {
                    "__score_normalized": {
                      "$divide": [
                        "$__score",
                        {
                          "$add": [
                            1,
                            "$__score",
                            {
                              "$abs": {
                                "$subtract": [
                                  1,
                                  {
                                    "$divide": [
                                      {
                                        "$strLenCP": "$Name"
                                      },
                                      {
                                        "$strLenCP": "{sender_name}"
                                      }
                                    ]
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  }
                },
                {
                  "$setWindowFields": {
                    "output": {
                      "__score_normalized_max": {
                        "$max": "$__score_normalized"
                      }
                    }
                  }
                },
                {
                  "$sort": {
                    "__score_normalized": -1
                  }
                },
                {
                  "$match": {
                    "$expr": {
                      "$cond": {
                        "if": {
                          "$or": [
                            {
                              "$and": [
                                {
                                  "$gt": [
                                    "$__score_normalized_max",
                                    0.9
                                  ]
                                },
                                {
                                  "$gt": [
                                    "$__score_normalized",
                                    0.9
                                  ]
                                }
                              ]
                            },
                            {
                              "$and": [
                                {
                                  "$gt": [
                                    "$__score_normalized_max",
                                    0.8
                                  ]
                                },
                                {
                                  "$lte": [
                                    "$__score_normalized_max",
                                    0.9
                                  ]
                                },
